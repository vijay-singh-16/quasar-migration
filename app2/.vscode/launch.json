{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "MS-Edge",
			"request": "launch",
			"type": "msedge",
			"url": "http://localhost:8080",
			"webRoot": "${workspaceFolder}",
			"skipFiles": [
				"<node_internals>/**"
			],
			"sourceMapPathOverrides": {
				"webpack://?:*/*": "${webRoot}/*"
			},
			"smartStep": true
		}
	]
}